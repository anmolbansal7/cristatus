{
    "name": "cristatus",
    "version": "0.1.0",
    "description": "My first react typescript package",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anmolbansal7/cristatus.git"
    },
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./utils": "./dist/esm/utils/index.js",
        "./components": "./dist/esm/components/index.js"
    },
    "scripts": {
        "dev": "cd ./cristatus-app && pnpm run start",
        "build": "yarn build:esm && yarn build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
        "stylelint": "stylelint \"**/*.css\"",
        "test": "jest --config jest.config.js",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run eslint"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
        "react",
        "typescript",
        "awesome-project"
    ],
    "author": "anmolbansal7",
    "license": "MIT",
    "devDependencies": {
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.3",
        "@types/react": "^18.2.20",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "eslint": "^8.47.0",
        "jest": "^29.6.2",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "^29.6.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "stylelint": "^15.10.2",
        "stylelint-config-recommended": "^13.0.0",
        "ts-jest": "^29.1.1",
        "typescript": "^5.1.6",
        "tslib": "^2.6.1"
    }
}
